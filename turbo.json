{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "**/dist/**"
      ]
    },
    "test": {
      "outputs": []
    },
    "prerelease": {
      "outputs": []
    },
    "postrelease": {
      "outputs": []
    }
  },
  "globalDependencies": [
    ".env",
    "tsconfig.json",
    "vite.config.ts"
  ]
}
